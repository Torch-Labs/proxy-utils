{
  "name": "torchlabs-proxy-utils",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
